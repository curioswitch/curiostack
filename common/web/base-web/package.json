  "author": "Choko <choko@curioswitch.org>",
  "license": "MIT",
  "main": "build/index",
  "bin": {
    "build": "./build/dev/scripts/build.js",
    "check": "./build/dev/scripts/check.js",
    "fix": "./build/dev/scripts/fix.js",
    "generate": "./build/dev/scripts/generate.js",
    "generate-component": "./build/dev/scripts/generate-component.js",
    "generate-container": "./build/dev/scripts/generate-container.js",
    "generate-package": "./build/dev/scripts/generate-package.js",
    "start": "./build/dev/scripts/start.js",
    "test": "./build/dev/scripts/test.js"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "prebuild": "yarn run clean && yarn run lint",
    "build": "tsc --project tsconfig-dev.json  --outDir ./build/dev --noEmit false --target es2016 && copyfiles --exclude \"src/dev/**/*\" -u 1 \"src/**/*\" build && copyfiles -u 1 \"src/**/*.hbs\" build && copyfiles -u 1 \"src/**/src-template/**\" build && copyfiles -u 3 ../../../tsconfig.json build/dev/generators/package",
    "prepare": "yarn run build",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-pattern *.d.ts --ignore-pattern **/src-template/**"
  },
