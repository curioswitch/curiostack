{
  "name": "@curiostack/base-web-dev",
  "version": "0.0.1",
  "description": "Curiostack's base dev dependencies for browser applications.",
  "main": "build/index.js",
  "repository": "curioswitch/curiostack",
  "author": "Choko <choko@curioswitch.org>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run build:clean",
    "build": "tsc --outDir ./build --noEmit false --lib es2016 --target es2016",
    "build:clean": "rimraf ./build",
    "//": "Normally we would lint before build, but we are actually building the lint config here.",
    "postbuild": "yarn run lint",
    "prepublish": "yarn run build",
    "lint": "yarn run tslint --project tsconfig.json"
  },
  "dependencies": {
    "@curiostack/base-node-dev": "0.0.1",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cache-loader": "1.2.0",
    "fork-ts-checker-webpack-plugin": "0.2.10",
    "html-webpack-plugin": "2.30.1",
    "http-proxy-middleware": "0.17.4",
    "thread-loader": "1.1.2",
    "ts-loader": "3.2.0",
    "tslint-config-airbnb": "5.4.2",
    "tslint-react": "3.3.3",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "2.0.3",
    "webpack-hot-middleware": "2.21.0"
  },
  "devDependencies": {
    "//": "Declare bin transitive dependencies here until https://github.com/yarnpkg/yarn/pull/4848",
    "rimraf": "2.6.2",
    "tslint": "5.8.0",
    "typescript": "2.6.2"
  }
}
